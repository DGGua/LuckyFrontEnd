{
  "compilerOptions": {
    "outDir": "./built",
    "allowJs": true,
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "./src/common/**/*",
    "./src/components/**/*",
    "./src/config/**/*",
    "./src/pages/Detail/**/*",
    "./src/pages/Home/**/*",
    "./src/pages/Login/**/*",
    "./src/pages/MyWish/**/*",
    "./src/pages/Send/**/*",
    "./src/pages/Wishes/**/*",
    "./src/static/**/*",
    "./src/utils/**/*"
  ]
}
